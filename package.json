{
  "name": "ts-retrofit",
  "version": "1.18.0",
  "description": "A declarative and axios based retrofit implementation for JavaScript and TypeScript.",
  "repository": "https://github.com/nullcc/ts-retrofit",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "keywords": [
    "retrofit",
    "ts",
    "js",
    "axios"
  ],
  "bugs": "https://github.com/nullcc/ts-retrofit/issues",
  "scripts": {
    "test": "jest --coverage",
    "tslint": "tslint -c tslint.json --project . src/**/*.ts test/**/*.test.ts",
    "build": "tsc",
    "make-badges": "npm run test:badges",
    "prepare": "npm run build",
    "prepublishOnly": "npm run tslint",
    "preversion": "npm run tslint",
    "postversion": "git push && git push --tags",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage && jest-coverage-badges"
  },
  "author": "Ethan Zhang",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run make-badges && git add README.md"
    }
  },
  "dependencies": {
    "axios": "^1.1.3",
    "form-data": "^2.5.1",
    "qs": "^6.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^29.2.0",
    "@types/multer": "^1.3.10",
    "@types/qs": "^6.5.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-coverage-badges": "^1.1.2",
    "multer": "^1.4.2",
    "ts-jest": "^29.0.3",
    "tslint": "^5.19.0",
    "typescript": "^4.8.4"
  }
}
